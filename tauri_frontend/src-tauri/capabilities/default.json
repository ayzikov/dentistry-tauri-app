{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "http:allow-fetch",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://127.0.0.1:8000/*"
        }
      ]
    },
    "http:default"
  ]
}